{
  "extends": ["tslint:latest", "tslint-eslint-rules"],
  "rules": {
    "no-console": false,
    "comma-dangle": false,
    "space-before-function-paren": false,
    "func-names": false,
    "prefer-arrow-callback": false,
    "semi": false,
    "no-unused-vars": false,
    "arrow-parens": false,
    "array-bracket-spacing": false,
    "indent": false,
    "arrow-body-style": false,
    "object-curly-spacing": false,
    "import/newline-after-import": false,
    "function-paren-newline": false,
    "quotes": false,
    "spaced-comment": false,
    "padded-blocks": false,
    "block-spacing": false,
    "no-trailing-spaces": false,
    "camelcase": false,
    "quotemark": [true, "single", "backtick", "jsx-double"],
    "trailing-comma": false,
    "interface-name": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "semicolon": false,
    "no-namespace": false,
    "no-empty": false,
    "no-implicit-dependencies": [true, "dev"]
  }
}
